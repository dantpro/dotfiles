#!/usr/local/bin/expect -f

set upFile [lindex $argv 2]
set dir [lindex $argv 3]

spawn cadaver https://dav.box.com/dav
expect "dav:/dav/>"
send "lcd /home/bucky/$dir\n"
expect "dav:/dav/>"
send "cd $dir\n"
expect "dav:/dav/$dir/>"
send "get $upFile $upFile\n"
expect "dav:/dav/$dir/>"
send "quit\n"
