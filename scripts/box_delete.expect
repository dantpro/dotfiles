#!/usr/local/bin/expect -f

set user [lindex $argv 0]
set pass [lindex $argv 1]
set upFile [lindex $argv 2]
set dir [lindex $argv 3]

spawn cadaver https://dav.box.com/dav
expect "Username:"
send $user\n
expect "Password:"
send $pass\n
expect "dav:/dav/>"
send "cd $dir\n"
expect "dav:/dav/$dir/>"
send "rm $upFile*\n"
expect "dav:/dav/$dir/>"
send "quit\n"
